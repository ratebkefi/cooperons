<script type="text/javascript">
    $(document).ready(function(){

        $('.btn-inscription').addClass('animated zoomInUp');

        $(window).on('scroll', function() {

            var navHeight = 510; // custom nav height 478
            if ( $('.category-video').length ) { var blocvideoTop = $('.category-video').offset().top; var blocvideoHeight = $('.category-video').height()+ 200;} // custom nav height
            if ( $('.category-fond1').length ) { var bloc1Top = $('.category-fond1').offset().top; var blocfond1Height = $('.category-fond1').height()+ 200;} // custom nav height
            if ( $('.category-fond2').length ) { var bloc2Top = $('.category-fond2').offset().top; var blocfond2Height = $('.category-fond2').height()+ 200;} // custom nav height
            if ( $('.category-fond3').length ) { var bloc3Top = $('.category-fond3').offset().top; var blocfond3Height = $('.category-fond3').height()+ 200;} // custom nav height
            if ( $('.category-fond4').length ) { var bloc4Top = $('.category-fond4').offset().top; var blocfond4Height = $('.category-fond4').height()+ 200;} // custom nav height


            if($(window).scrollTop() > 465 && !$('header').hasClass('goToTopFaq')){ $('header').addClass('goToTop10'); } else{ $('header').removeClass('goToTop10');}
            if($(window).scrollTop() > navHeight && !$('header').hasClass('goToTopFaq')){ $('header').removeClass('goToTop10'); $('header').addClass('goToTop'); } else{ $('header').removeClass('goToTop');}
            if($(window).scrollTop() > navHeight){ $('.category-video').addClass('animate'); } else { $('.category-video').removeClass('animate'); }
            if($(window).scrollTop() > navHeight){ $('.category-fond2').addClass('animate'); }else{ $('.category-fond2').removeClass('animate'); }

            if ($(window).scrollTop() > blocvideoTop - blocvideoHeight) {$('.category-video .row').addClass('animated fadeIn');}
            if ($(window).scrollTop() > bloc1Top - blocfond1Height) {$('.category-fond1 .row').addClass('animated fadeInRight');}
            if ($(window).scrollTop() > bloc2Top - blocfond2Height) {$('.category-fond2 .row').addClass('animated fadeInLeft');}
            if ($(window).scrollTop() > bloc3Top - blocfond3Height) {$('.category-fond3 .row').addClass('animated fadeInRight');}
            if ($(window).scrollTop() > bloc4Top - blocfond4Height) {$('.category-fond4 .row').addClass('animated fadeInLeft');}

        });


    });
</script>


